title = {{Experimental comparative analysis of adaptive fuzzy logic
controllers}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0036494537\&partnerID=40\&md5=2ed7c7446943d95e025b97fa66d79bfe http://www.scopus.com/inward/record.url?eid=2-s2.0-0036494537\&partnerID=tZOtx3y1},
