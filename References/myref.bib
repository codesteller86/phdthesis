keywords = {Fuzzy logic,grinding circuit (GC),optimization,process monitoring,soft-sensor,supervisory control},
month = jan,
number = {1},
pages = {162--175},
title = {{Intelligence-based supervisory control for optimal operation of a DCS-controlled grinding system}},
