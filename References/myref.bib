publisher = {Elsevier Ltd},
title = {{Design and simulation of a real-time implementable energy-efficient model-predictive cruise controller for electric vehicles}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84922838045\&partnerID=tZOtx3y1},
volume = {352},
year = {2015}
}
