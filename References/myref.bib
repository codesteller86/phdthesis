publisher = {Elsevier Ltd},
title = {{Adaptive sliding mode observer for sensor fault diagnosis of an industrial gas turbine}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84923357254\&partnerID=tZOtx3y1},
