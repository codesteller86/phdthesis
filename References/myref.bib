keywords = {Banknote image recognition,Fuzzy logic,Haar wavelet transformation,Linguistic variable,Neural networks},
number = {11},
pages = {1149--1155},
title = {{A method for banknote feature extraction based on Haar wavelet and fuzzy logic}},
