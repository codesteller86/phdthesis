keywords = {3-D point-of-regard,Attention map,eye tracking,scanpath},
pages = {531--536},
title = {{Estimating 3-D point-of-regard in a real environment using a head-mounted eye-tracking system}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84900350832\&partnerID=tZOtx3y1},
