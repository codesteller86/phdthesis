}
@article{Zhou2013,
abstract = {Optimizing the final grinding production indices (GPIs), which include$\backslash$nthe product particle size and the grinding production rate, to meet the$\backslash$noverall manufacturing performance requirements is the main function of$\backslash$nautomatic control of a grinding circuit (GC). However, the complex and$\backslash$ntime-varying nature of the GC process dictates that these GPIs cannot be$\backslash$noptimized solely by the lower-level distributed control systems (DCS),$\backslash$ntherefore an operator is often incorporated to manually determine the$\backslash$nset-points for the DCS using his/her operational experience. With a$\backslash$nhuman being involved, the performance and even the safety and stability$\backslash$nof the GC operation is subject to human errors. Focusing on this$\backslash$npractical challenge, this paper proposes an intelligence-based$\backslash$nsupervisory control strategy that consists of a control loop set-point$\backslash$noptimization module, an artificial neural network-based soft-sensor$\backslash$nmodule, a fuzzy logic-based dynamic adjustor, and an expert-based$\backslash$noverload diagnosis and adjustment module to perform the control tasks$\backslash$nfor the GC system. This hybrid system can automatically adjust the$\backslash$nset-points for the DCS-controlled grinding system in response to the$\backslash$nchanges in boundary conditions or the imminent overload conditions,$\backslash$nthereby eliminating the need for an operator. Practical applications$\backslash$nhave shown the validity and effectiveness of the proposed approach.},
address = {State Key Laboratory of Synthetical Automation for Process Industries, Northeastern University, Shenyang 110819, China},
