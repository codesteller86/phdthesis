@article{Chen2003a,
abstract = { In general, fuzzy control design for stochastic nonlinear systems is still difficult since the fuzzy bases are stochastic so as to increase the difficulty and complexity of the fuzzy tracking control design. In this study, a fuzzy stochastic moving-average model with control input (fuzzy ARMAX model) is introduced to describe nonlinear stochastic systems. From the fuzzy ARMAX model, a fuzzy one-step ahead prediction model is developed. Based on a fuzzy one-step ahead prediction stochastic model, optimal design algorithms are proposed to achieve the optimal tracking of nonlinear stochastic systems. In this study, the minimum variance tracking control, generalized minimum variance tracking control, and the optimal model reference tracking control are developed for stochastic fuzzy systems. We construct some basic stability conditions for general stochastic fuzzy systems and use these conditions to verify the stability of the fuzzy tracking control systems. Finally, two simulation examples are given to indicate the performance of the proposed methods.},
author = {Chen, Bor Sen and Lee, Bore Kuen and Guo, Ling Bin},
doi = {10.1109/TFUZZ.2003.819836},
issn = {10636706},
journal = {IEEE Transactions on Fuzzy Systems},
keywords = {Fuzzy ARMAX model,Fuzzy predictive model,Stochastic fuzzy system},
number = {6},
pages = {796--813},
title = {{Optimal Tracking Design for Stochastic Fuzzy Systems}},
volume = {11},
year = {2003}
}
@article{Gao2013,
abstract = {This paper investigates the universal fuzzy model and universal fuzzy controller problems for stochastic nonaffine nonlinear systems. The underlying mechanism of stochastic fuzzy logic is first discussed, and a stochastic generalized fuzzy model with new stochastic fuzzy rule base is then given. Based on their function approximation capability, these kinds of stochastic generalized fuzzy models are shown to be universal fuzzy models for stochastic nonaffine nonlinear systems under some sufficient conditions. An approach to stabilization controller design for stochastic nonaffine nonlinear systems is then developed through their stochastic generalized Takagi-Sugeno (T-S) fuzzy approximation models. Then, the results of universal fuzzy controllers for two classes of stochastic nonlinear systems, along with constructive procedures to obtain the universal fuzzy controllers, are also provided, respectively. Finally, a numerical example is presented to illustrate the effectiveness of the proposed approach.},
author = {Gao, Qing and Feng, Gang and Wang, Yong and Qiu, Jianbin},
doi = {10.1109/TFUZZ.2012.2213823},
isbn = {1063-6706},
issn = {10636706},
journal = {IEEE Transactions on Fuzzy Systems},
keywords = {Stochastic nonaffine nonlinear systems (SNNS),stochastic Takagi-Sugeno (T-S) fuzzy models,universal fuzzy controllers,universal fuzzy models},
number = {2},
pages = {328--341},
title = {{Universal fuzzy models and universal fuzzy controllers for stochastic nonaffine nonlinear systems}},
volume = {21},
year = {2013}
}
@article{Wang1993,
abstract = {A direct adaptive fuzzy controller that does not require an$\backslash$naccurate mathematical model of the system under control, is capable of$\backslash$nincorporating fuzzy if-then control rules directly into the controllers,$\backslash$nand guarantees the global stability of the resulting closed-loop system$\backslash$nin the sense that all signals involved are uniformly bounded is$\backslash$ndeveloped. The specific formula for the bounds is provided, so that$\backslash$ncontroller designers can determine the bounds based on their$\backslash$nrequirements. The direct adaptive fuzzy controller is used to regulate$\backslash$nan unstable system to the origin and to control the Duffing chaotic$\backslash$nsystem to track a trajectory. The simulation results show that the$\backslash$ncontroller worked without using any fuzzy control rules, and that after$\backslash$nfuzzy control rules were incorporated the adaptation speed became much$\backslash$nfaster. It is shown explicitly how the supervisory control forces the$\backslash$nstate to remain within the constraint set and how the adaptive fuzzy$\backslash$ncontroller learns to regain control},
